{
  "compilerOptions": {
    "baseUrl": "", // This must be specified if "paths" is.
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@config/*": [
        "src/configs/*"
      ],
      "@features/*": [
        "src/features/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/checkoutpayement/payement.js"
  ],
  "exclude": [
    "node_modules"
  ]
}